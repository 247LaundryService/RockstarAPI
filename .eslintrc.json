{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "no-await-in-loop": "off",
        "no-plusplus": "off",
        "no-param-reassign": "off",
        "no-console": "off",
        "max-len":"off"
    }
}